# Streamlit Secrets Configuration for Cloud Deployment
# Copy this file to .streamlit/secrets.toml and fill in your actual values

# Gemini AI API Key
GEMINI_API_KEY = "your_gemini_api_key_here"

# Google OAuth Configuration
[google_oauth]
client_id = "your_google_client_id_here"
client_secret = "your_google_client_secret_here"
redirect_uri = "https://your-app-name.streamlit.app"

# Token Encryption (Optional - for enhanced security)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
encryption_key = "your_base64_encryption_key_here"
